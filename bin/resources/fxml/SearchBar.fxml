<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vac_georef.controller.SearchBarController">
	<children>
		<HBox prefHeight="20.0" prefWidth="243.0">
			<children>
				<TextField fx:id="textSearch" onInputMethodTextChanged="#dynamicSearch" />
				<Button fx:id="searchButton" mnemonicParsing="false" onAction="#staticSearch" text="Search">
					<HBox.margin>
						<Insets left="5.0" />
					</HBox.margin>
				</Button>
			</children>
			<VBox.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</VBox.margin>
		</HBox>
		<ListView fx:id="listView" prefHeight="420.0" prefWidth="200.0">
			<VBox.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</VBox.margin>
		</ListView>
	</children>
</VBox>
